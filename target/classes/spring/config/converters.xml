<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<bean class="converters.ActorToStringConverter"/>
		<bean class="converters.AdministratorToStringConverter"/>
		<bean class="converters.ArtistToStringConverter"/>
		<bean class="converters.ArtworkToStringConverter"/>
		<bean class="converters.AuthorityToStringConverter"/>
		<bean class="converters.CartToStringConverter"/>
		<bean class="converters.CustomerToStringConverter"/>
		<bean class="converters.OrderToStringConverter"/>
		<bean class="converters.OrderLineToStringConverter"/>
		<bean class="converters.ProfitToStringConverter"/>
		<bean class="converters.PurchaserToStringConverter"/>
		<bean class="converters.ReviewToStringConverter"/>
		<bean class="converters.TaxToStringConverter"/>
		<bean class="converters.UserAccountToStringConverter"/>
		<bean class="converters.StringToActorConverter"/>
		<bean class="converters.StringToAdministratorConverter"/>
		<bean class="converters.StringToArtistConverter"/>
		<bean class="converters.StringToArtworkConverter"/>
		<bean class="converters.StringToAuthorityConverter"/>
		<bean class="converters.StringToCartConverter"/>
		<bean class="converters.StringToCustomerConverter"/>
		<bean class="converters.StringToOrderConverter"/>
		<bean class="converters.StringToOrderLineConverter"/>
		<bean class="converters.StringToProfitConverter"/>
		<bean class="converters.StringToPurchaserConverter"/>
		<bean class="converters.StringToReviewConverter"/>
		<bean class="converters.StringToTaxConverter"/>
		<bean class="converters.StringToUserAccountConverter"/>
	</util:list>
	
</beans>